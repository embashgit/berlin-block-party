{
  "name": "berlin-block-party",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "npm run config -- --environment=dev && ng serve",
    "config": "ts-node ./src/assets/scripts/setEnv.ts",
    "start": "npm run config -- --environment=dev && node server.js",
    "build:prod": "npm run config -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "heroku-postbuild": "ng build --aot",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.4",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "assert": "^2.0.0",
    "express": "^4.18.1",
    "ngx-pagination": "^6.0.2",
    "path": "^0.12.7",
    "rxjs": "~7.5.0",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "ts-node": "^10.8.2",
    "tslib": "^2.3.0",
    "xml-js": "^1.6.11",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular-eslint/builder": "14.0.0",
